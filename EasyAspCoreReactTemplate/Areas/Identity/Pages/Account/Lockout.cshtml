@page
@model LockoutModel
@{
    ViewData["Title"] = "Locked out";
}

<section class="max-w-xl rounded-3xl border border-rose-500/30 bg-rose-500/10 p-8">
    <h1 class="text-2xl font-semibold text-rose-200">@ViewData["Title"]</h1>
    <p class="mt-2 text-sm text-rose-100">This account has been locked out, please try again later.</p>
</section>
